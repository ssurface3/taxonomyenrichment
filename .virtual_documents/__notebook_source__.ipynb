# --- VS CODE REMOTE TUNNEL SETUP ---
import os
import time
import subprocess

# 1. Install the VS Code CLI
if not os.path.exists("code"):
    print("‚¨áÔ∏è Downloading VS Code CLI...")
    get_ipython().getoutput("curl -Lk 'https://code.visualstudio.com/sha/download?build=stable&os=cli-alpine-x64' --output vscode_cli.tar.gz")
    get_ipython().getoutput("tar -xf vscode_cli.tar.gz")
    print("‚úÖ Installed.")

# 2. Define a unique name for this machine
machine_name = "kaggle-gpu-worker"

print(f"\nüöÄ STARTING TUNNEL: '{machine_name}'")
print("--------------------------------------------------")
print("1. COPY the code typically looking like 'ABCD-1234' from the output below.")
print("2. CLICK the link https://github.com/login/device")
print("3. PASTE the code to authorize.")
print("--------------------------------------------------")

# 3. Run the Tunnel
# We use subprocess to stream the output so you can see the login code
cmd = f"./code tunnel --accept-server-license-terms --name {machine_name}"
process = subprocess.Popen(cmd, shell=True, stdout=subprocess.PIPE, stderr=subprocess.STDOUT, text=True)

# Stream output line by line
for line in process.stdout:
    print(line, end="")
